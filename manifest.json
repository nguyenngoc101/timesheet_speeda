{
  "manifest_version": 2,

  "name": "TimeShit",
  "description": "This extension allows the user to get timesheet in wsm framgia system",
  "version": "2.0",

  "key": "",
  // "browser_action": {
  //   "default_icon": "icon.png",
  //   "default_popup": "popup.html"
  // },
  "permissions": [
    // "activeTab",
    // "storage",
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],

  "content_scripts": [
    {
      "matches": ["https://wsm.framgia.vn/*"],
      "js": ["getPagesSource.js"],
      "run_at": "document_end"
    }
  ],

  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "oauth2": {
    // client_id below is specifc to the application key. Follow the
    // documentation to obtain one for your app.
    "client_id": "883269279766-7l87a496q7e4uudo3clso3gaeaufmcfa.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/spreadsheets"]
  }
}
